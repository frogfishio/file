{
  "name": "@frogfish/file",
  "version": "1.0.0",
  "description": "",
  "main": "gulpfile.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "DEBUG=engine*,service*,request* ENGINE_ROOT=$PWD mocha --exit --recursive build/debug/test",
    "watch": "gulp prepare-debug; tsc -w",
    "start": "DEBUG=engine*,service:*  tsc-watch --onSuccess \"node node_modules/@frogfish/engine/engine -c test/service.yaml --root $PWD\"",
    "build": "gulp build",
    "clean-build": "gulp clean; gulp build",
    "dist": "npm version patch; gulp clean; gulp build; cd build/release; npm publish --access public; git push origin master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frogfishio/file.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/frogfishio/file/issues"
  },
  "homepage": "https://github.com/frogfishio/file#readme",
  "dependencies": {
    "@frogfish/engine": "^2.0.3",
    "busboy": "^0.3.1"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.10.59",
    "chai": "^4.2.0",
    "chai-like": "^1.1.1",
    "chai-things": "^0.2.0",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^5.0.1",
    "mkdirp": "^0.5.1",
    "typescript": "^3.7.5"
  }
}
